            meaning
        FROM
            fnd_lookups
        WHERE
                lookup_type = ''YES_NO''
            AND ( enabled_flag = ''Y''
                  OR end_date_active IS NOT NULL )
            AND lookup_code = pcr.primary_contact_flag
    )  primary_contact_flag,
