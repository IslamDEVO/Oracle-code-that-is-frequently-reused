            meaning
        FROM
            hr_lookups
        WHERE
                lookup_type = ''PQH_GENDER''
            AND ( enabled_flag = ''Y''
                  OR end_date_active IS NOT NULL )
            AND lookup_code = papf2.sex
    )  gender,
