    --PPM.ATTRIBUTE_CATEGORY , PPM.ATTRIBUTE1 , PPM.ATTRIBUTE2 , PPM.ATTRIBUTE3 , PPM.ATTRIBUTE4 , PPM.ATTRIBUTE5 , PPM.ATTRIBUTE6 , PPM.ATTRIBUTE7 , PPM.ATTRIBUTE8 , PPM.ATTRIBUTE9 , PPM.ATTRIBUTE10 , PPM.ATTRIBUTE11 , PPM.ATTRIBUTE12 , PPM.ATTRIBUTE13 , PPM.ATTRIBUTE14 , PPM.ATTRIBUTE15 , PPM.ATTRIBUTE16 , PPM.ATTRIBUTE17 , PPM.ATTRIBUTE18 , PPM.ATTRIBUTE19 , PPM.ATTRIBUTE20 , 
    PPM.LAST_UPDATE_DATE , PPM.LAST_UPDATED_BY , PPM.LAST_UPDATE_LOGIN , PPM.CREATED_BY , PPM.CREATION_DATE , OPMTL.ORG_PAYMENT_METHOD_NAME , OPM.PAYMENT_TYPE_ID , OPM.CURRENCY_CODE , OPM.DEFINED_BALANCE_ID , PTTL.PAYMENT_TYPE_NAME , PT.TERRITORY_CODE , PT.CATEGORY , PT.PRE_VALIDATION_REQUIRED , C.COMMENT_TEXT , PEA.PRENOTE_DATE , PEA.ID_FLEX_NUM , PBD.BALANCE_TYPE_ID , ppm.person_id , ppm.run_type_id , PLR.RULE_MODE C_BANK_STRUCTURE , PA.PAYROLL_ID , FF.EFFECTIVE_DATE , ppm.PPM_INFORMATION_CATEGORY 
    --,ppm.PPM_INFORMATION1 , ppm.PPM_INFORMATION2 , ppm.PPM_INFORMATION3 , ppm.PPM_INFORMATION4 , ppm.PPM_INFORMATION5 , ppm.PPM_INFORMATION6 , ppm.PPM_INFORMATION7 , ppm.PPM_INFORMATION8 , ppm.PPM_INFORMATION9 , ppm.PPM_INFORMATION10 , ppm.PPM_INFORMATION11 , ppm.PPM_INFORMATION12 , ppm.PPM_INFORMATION13 , ppm.PPM_INFORMATION14 , ppm.PPM_INFORMATION15 , ppm.PPM_INFORMATION16 , ppm.PPM_INFORMATION17 , ppm.PPM_INFORMATION18 , ppm.PPM_INFORMATION19 , ppm.PPM_INFORMATION20 , ppm.PPM_INFORMATION21 , ppm.PPM_INFORMATION22 , ppm.PPM_INFORMATION23 , ppm.PPM_INFORMATION24 , ppm.PPM_INFORMATION25 , ppm.PPM_INFORMATION26 , ppm.PPM_INFORMATION27 , ppm.PPM_INFORMATION28 , ppm.PPM_INFORMATION29 , ppm.PPM_INFORMATION30, ppm.OBJECT_VERSION_NUMBER 
FROM PAY_ORG_PAYMENT_METHODS_F_TL OPMTL, PAY_PERSONAL_PAYMENT_METHODS_F PPM, PAY_ORG_PAYMENT_METHODS_F OPM, PAY_PAYMENT_TYPES_TL PTTL, PAY_PAYMENT_TYPES PT, HR_COMMENTS C, PAY_EXTERNAL_ACCOUNTS PEA, PAY_DEFINED_BALANCES PBD, PER_ALL_ASSIGNMENTS_F PA, PAY_LEGISLATION_RULES PLR, FND_SESSIONS FF ,HR_LOOKUPS H
WHERE 
    PT.PAYMENT_TYPE_ID = PTTL.PAYMENT_TYPE_ID 
    AND PTTL.LANGUAGE = userenv(''LANG'') 
    AND OPM.ORG_PAYMENT_METHOD_ID = OPMTL.ORG_PAYMENT_METHOD_ID 
    AND OPMTL.LANGUAGE = userenv(''LANG'') 
    AND FF.SESSION_ID = USERENV(''SESSIONID'') 
    AND FF.EFFECTIVE_DATE BETWEEN PPM.EFFECTIVE_START_DATE AND PPM.EFFECTIVE_END_DATE 
    AND FF.EFFECTIVE_DATE BETWEEN OPM.EFFECTIVE_START_DATE AND OPM.EFFECTIVE_END_DATE 
    AND FF.EFFECTIVE_DATE BETWEEN PA.EFFECTIVE_START_DATE AND PA.EFFECTIVE_END_DATE 
    AND PPM.ORG_PAYMENT_METHOD_ID = OPM.ORG_PAYMENT_METHOD_ID 
    AND OPM.PAYMENT_TYPE_ID = PT.PAYMENT_TYPE_ID 
    AND PPM.COMMENT_ID = C.COMMENT_ID(+) 
    AND PPM.EXTERNAL_ACCOUNT_ID = PEA.EXTERNAL_ACCOUNT_ID (+) 
    AND OPM.DEFINED_BALANCE_ID = PBD.DEFINED_BALANCE_ID (+) 
    AND ( (ppm.assignment_id is not null and PPM.ASSIGNMENT_ID = PA.ASSIGNMENT_ID) or (ppm.person_id is not null and PPM.person_id = pa.person_id) ) 
    AND PT.TERRITORY_CODE = PLR.LEGISLATION_CODE(+) 
    AND PLR.RULE_TYPE(+) = ''E''
    AND H.LOOKUP_CODE=PEA.segment1
    AND H.LOOKUP_TYPE = ''SA_BANKS'' 
    AND H.ENABLED_FLAG = ''Y''
--    AND PPM.assignment_id=97213
    AND PA.person_id = #PER_PERSON_ID#',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getEmpVisitorReqInfo','select a.full_name "fullName", a.employee_number "employee_number", a.email_address "email", HLA.LOCATION_CODE "location", pg.name "grade", NVL(xvzep.NAME,''NONE'') "position"
from
    per_all_people_f a,
    HR_LOCATIONS_ALL HLA,
    PER_ALL_AsSIGNMENTS_F PAF,
    per_grades pg,
    XX_V_ZAIN_EMP_POSITION xvzep
where
    a.PERSON_ID= #PER_PERSON_ID#
    AND a.effective_end_date=(select max(effective_end_date) from per_all_people_f where person_id=a.person_id)
    AND PAF.LOCATION_ID=HLA.LOCATION_ID
    AND PAF.PERSON_ID= #PER_PERSON_ID#
    AND PAF.PRIMARY_FLAG=''Y''
    AND PAF.grade_Id = pg.grade_Id
    AND trim(SYSDATE) BETWEEN PAF.EFFECTIVE_START_DATE AND PAF.EFFECTIVE_END_DATE
    AND xvzep.PERSON_ID= #PER_PERSON_ID#',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getBusinessMissionDefaults','SELECT DISTINCT
    gcc.segment2 "empCostCenter", 
    l.location_code "locationCode", 
    papf.full_name "fullName", 
    papf.date_of_birth "dateOfBirth",
    paaf.ass_attribute4 "empLevel", 
    papf.national_identifier "nationalIdentifier",
    ppei.pei_information1 "passportNumber",
    ppei.pei_information3 "passDateIssue",
    ppei.pei_information4 "passDateExpire"
FROM
    per_all_people_f        papf
    left join per_all_assignments_f   paaf   on (paaf.person_id = papf.person_id)
    left join gl_code_combinations    gcc    on (gcc.code_combination_id = paaf.default_code_comb_id)
    left join hr_locations            l      on (l.location_id = paaf.location_id)
    left join per_people_extra_info   ppei   on (ppei.person_id = papf.person_id
                                                AND ppei.pei_information_category = ''SA_PASSPORT''
                                                AND sysdate BETWEEN to_date(ppei.pei_information3, ''YYYY/MM/DD HH24:MI:SS'') AND to_date(ppei.pei_information4, ''YYYY/MM/DD HH24:MI:SS'')
                                                AND ppei.last_update_date = (
                                                    SELECT
                                                        MAX(last_update_date)
                                                    FROM
                                                        per_people_extra_info ppeii
                                                    WHERE
                                                        ppeii.pei_information_category = ''SA_PASSPORT''
                                                        AND ppeii.person_id = ppei.person_id
                                                ))
WHERE
    papf.PERSON_ID = #PER_PERSON_ID#
    AND sysdate BETWEEN papf.effective_start_date AND papf.effective_end_date
    AND sysdate BETWEEN paaf.effective_start_date AND paaf.effective_end_date',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getMgmntPlans','SELECT plan_id, plan_name, start_date, end_date, status_code
--   INTO P_DEADLINE
FROM PER_PERF_MGMT_PLANS
where 1=1','plan_name|start_date|end_date|status_code');
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getPersonAddresses',' SELECT business_group_id,
         d_style,
         d_address_type,
         primary_flag,
         date_from,
         date_to,
         address_line1,
         address_line2,
         address_line3,
         (SELECT MEANING
            FROM HR_LOOKUPS H
           WHERE     LOOKUP_TYPE = ''SA_CITY''
                 AND ENABLED_FLAG = ''Y''
                 AND APPLICATION_ID = 800
                 AND LOOKUP_CODE = town_or_city
                 AND (ENABLED_FLAG = ''Y'' OR END_DATE_ACTIVE IS NULL))
            town_or_city,
         town_or_city CODE,
         region_2,
         postal_code,
         region_1,
         telephone_number_1,
         telephone_number_2,
         region_3,
         style,
         addr_attribute1,
         addr_attribute_category,
         add_information13,
         add_information14,
         telephone_number_3,
         comments,
         person_id,
         address_type,
         address_id,
         (SELECT TERRITORY_SHORT_NAME
            FROM FND_TERRITORIES_VL
           WHERE OBSOLETE_FLAG <> ''Y'' AND TERRITORY_CODE = country)
            COUNTRY_NAME,
         country,
         object_version_number
    FROM per_addresses_v
   WHERE per_addresses_v.person_id = #PER_PERSON_ID# AND business_group_id+0 = FND_PROFILE.VALUE(''PER_BUSINESS_GROUP_ID'')
ORDER BY primary_flag DESC, date_from DESC','primary_flag');
