            pose1.organization_id_child = (
                SELECT max (
                    pose.organization_id_child )
                FROM
                    per_org_structure_elements   pose,
                    hr_all_organization_units    haou
                WHERE
                    haou.organization_id = pose.organization_id_child
                    AND haou.type = ''DIV''
                    AND ROWNUM < 2
                START WITH
                    pose.organization_id_child = (
                        SELECT max(
                            paaf2.organization_id )
                        FROM
                            per_all_assignments_f paaf2
