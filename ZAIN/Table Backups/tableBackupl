Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getQualifications','select  
qua.QUALIFICATION_ID
, qua.PERSON_ID
, qua.TITLE
, qua.GRADE_ATTAINED
, qua.STATUS
, qua.START_DATE "StudyStartDate"
, qua.END_DATE "ActualCompletionDate"
, qua.EXPIRY_DATE
, qua.COMMENTS
, qua.QUALIFICATION_TYPE_ID
, qua.ATTENDANCE_ID
, qua.PARTY_ID
, est.ESTABLISHMENT_ID
, est_attn.ESTABLISHMENT "EducationalInstitute"
, est.name
from per_qualifications qua
left join PER_ESTABLISHMENT_ATTENDANCES est_attn on (qua.ATTENDANCE_ID = est_attn.ATTENDANCE_ID)
left join PER_ESTABLISHMENTS est on (est_attn.ESTABLISHMENT_ID = est.ESTABLISHMENT_ID)
where qua.person_id = #PER_PERSON_ID#',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getObjectives','SELECT po.OBJECTIVE_ID, po.NAME, po.OWNING_PERSON_ID, po.APPRAISAL_ID, po.SCORECARD_ID
        ,pps.SCORECARD_NAME, pps.PERSON_ID, pps.PLAN_ID
FROM PER_OBJECTIVES po
join PER_PERSONAL_SCORECARDS pps
on (po.SCORECARD_ID = pps.SCORECARD_ID)
where 1=1','pps.PLAN_ID|po.OWNING_PERSON_ID');
