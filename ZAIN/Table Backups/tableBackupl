Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('mainInfo','select p.person_id, p.last_name, p.employee_number, p.FIRST_NAME, p.start_date hire_date,
        u.EMAIL_ADDRESS, u.user_id, u.user_name, u.employee_id,
        assmt.position_id, assmt.job_id, assmt.grade_id, assmt.supervisor_id, assmt.organization_id,
        mgr.person_id manager_id, mgr.full_name manager_name, mgr.email_address manager_email,
        pos.name position_name, j.name job_name, g.name grade_name, o.name organization_name
from per_all_people_f p
    join fnd_user u
    on (p.person_id = u.employee_id)
    left join PER_ALL_ASSIGNMENTS_F assmt
    on (p.person_id = assmt.person_id)
    left join per_all_people_f mgr
    on (assmt.supervisor_id = mgr.person_id)
    left join hr_all_positions_f pos
    on (assmt.position_id = pos.position_id)
    left join per_jobs j
    on (assmt.job_id = j.job_id)
    left join per_grades g
    on (assmt.grade_id = g.grade_id)
    left join hr_all_organization_units o
    on (assmt.organization_id = o.organization_id)
where sysdate between p.effective_start_date and p.effective_end_date
    and sysdate between assmt.effective_start_date and assmt.effective_end_date
    and sysdate between pos.effective_start_date and pos.effective_end_date','p.PERSON_ID|p.EMPLOYEE_NUMBER|u.EMAIL_ADDRESS|p.FIRST_NAME');
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getPersonByRole','select p.person_id "personId", p.employee_number "personNumber", p.FIRST_NAME "firstName", p.last_name "lastName", p.full_name "fullName", p.start_date "hireDate",
        u.EMAIL_ADDRESS "email", u.user_id "ebsUserId", u.user_name "ebsUserName",
        assmt.position_id "PositionId", assmt.job_id "jobId", assmt.grade_id "gradeId", assmt.organization_id "departmentId",
        mgr.person_id "managerId", mgr.full_name "managerName", mgr.email_address "managerEmail",
        pos.name "PositionName", j.name "jobName", g.name "gradeName", o.name "departmentName"
,
        (select max(group_name) from pay_people_groups where PEOPLE_GROUP_ID = assmt.PEOPLE_GROUP_ID) "peopleGroup"
,
	''ACTIVE'' "active"
, ''TRUE'' "delegatedFlag"
from per_all_people_f p
    join fnd_user u
    on (p.person_id = u.employee_id)
    left join PER_ALL_ASSIGNMENTS_F assmt
    on (p.person_id = assmt.person_id)
    left join per_all_people_f mgr
    on (assmt.supervisor_id = mgr.person_id)
    left join hr_all_positions_f pos
    on (assmt.position_id = pos.position_id)
    left join per_jobs j
    on (assmt.job_id = j.job_id)
    left join per_grades g
    on (assmt.grade_id = g.grade_id)
    left join hr_all_organization_units o
    on (assmt.organization_id = o.organization_id)
where sysdate between p.effective_start_date and p.effective_end_date
    and sysdate between nvl(assmt.effective_start_date, sysdate) and nvl(assmt.effective_end_date, sysdate+1000)
    and sysdate between nvl(pos.effective_start_date, sysdate) and nvl(pos.effective_end_date, sysdate+1000)
    and sysdate between nvl(mgr.effective_start_date, sysdate) and nvl(mgr.effective_end_date, sysdate+1000)
    and sysdate between o.date_from and nvl(o.date_to, sysdate+1000)
    and sysdate between j.date_from and nvl(j.date_to, sysdate+1000)
    and sysdate between g.date_from and nvl(g.date_to, sysdate+1000)
    ----------------------------------------------
    and p.person_id = nvl(#PERSON_ID#, p.person_id)
    and rownum < nvl(#LIMIT#, 25)',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('loginInfo','select p.person_id, p.last_name, p.employee_number, p.FIRST_NAME,
        u.EMAIL_ADDRESS, u.user_id, u.user_name, u.employee_id
,
	apex_web_service.blob2clobbase64(img.image)
 image
from per_all_people_f p
join fnd_user u
on (p.person_id = u.employee_id)
left join per_images img
on (p.person_id = img.parent_id)
where sysdate between p.effective_start_date and p.effective_end_date','p.PERSON_ID|p.EMPLOYEE_NUMBER|u.EMAIL_ADDRESS|p.FIRST_NAME|p.LAST_NAME|u.USER_NAME');
