        u.EMAIL_ADDRESS "email", u.user_id "ebsUserId", u.user_name "ebsUserName",
        assmt.position_id "PositionId", assmt.job_id "jobId", assmt.grade_id "gradeId", assmt.organization_id "departmentId",
        mgr.person_id "managerId", mgr.full_name "managerName", mgr.email_address "managerEmail",
        pos.name "PositionName", j.name "jobName", g.name "gradeName", o.name "departmentName"
,
        (select max(group_name) from pay_people_groups where PEOPLE_GROUP_ID = assmt.PEOPLE_GROUP_ID) "peopleGroup"
,
	''ACTIVE'' "active"
from per_all_people_f p
    join fnd_user u
    on (p.person_id = u.employee_id)
    left join PER_ALL_ASSIGNMENTS_F assmt
    on (p.person_id = assmt.person_id)
    left join per_all_people_f mgr
    on (assmt.supervisor_id = mgr.person_id)
    left join hr_all_positions_f pos
    on (assmt.position_id = pos.position_id)
    left join per_jobs j
    on (assmt.job_id = j.job_id)
    left join per_grades g
    on (assmt.grade_id = g.grade_id)
    left join hr_all_organization_units o
    on (assmt.organization_id = o.organization_id)
where sysdate between p.effective_start_date and p.effective_end_date
    and sysdate between nvl(assmt.effective_start_date, sysdate) and nvl(assmt.effective_end_date, sysdate+1000)
    and sysdate between nvl(pos.effective_start_date, sysdate) and nvl(pos.effective_end_date, sysdate+1000)
    and sysdate between nvl(mgr.effective_start_date, sysdate) and nvl(mgr.effective_end_date, sysdate+1000)
    and sysdate between o.date_from and nvl(o.date_to, sysdate+1000)
    and sysdate between j.date_from and nvl(j.date_to, sysdate+1000)
    and sysdate between g.date_from and nvl(g.date_to, sysdate+1000)
    ----------------------------------------------
    and p.person_id = nvl(#PERSON_ID#, p.person_id)
    and assmt.supervisor_id = nvl(#REQUESTER_ID#, assmt.supervisor_id)
    and rownum < nvl(null, 50)',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getUserInfo','select p.person_id "personId", p.employee_number "personNumber", p.sex "gender", p.FIRST_NAME "firstName", p.last_name "lastName",
p.PREVIOUS_LAST_NAME "arabicName", p.PER_INFORMATION3 "arabicNameSeg1", p.PER_INFORMATION4 "arabicNameSeg2", p.PER_INFORMATION5 "arabicNameSeg3", p.PER_INFORMATION6 "arabicNameSeg4",
regexp_substr(p.nationality, ''[^_]+$'', 1, 1) "nationality", p.ATTRIBUTE2 "nationalityArabic", p.full_name "fullName", p.start_date "hireDate", p.MARITAL_STATUS "maritalStatus", p.date_of_birth "birthDate", floor((sysdate-p.date_of_birth)/30/12) "age",
(select phone_number from per_phones where phone_type=''H1'' and parent_id=p.person_id) "homePhone",
(select phone_number from per_phones where phone_type=''M'' and parent_id=p.person_id) "mobilePhone",
(select phone_number from per_phones where phone_type=''W'' and parent_id=p.person_id) "workPhone",
(select phone_number from per_phones where phone_type=''W1'' and parent_id=p.person_id) "workPhone2",
(select phone_number from per_phones where phone_type=''WF'' and parent_id=p.person_id) "workFax",
(select phone_number from per_phones where phone_type=''HF'' and parent_id=p.person_id) "homeFax",
u.EMAIL_ADDRESS "email", u.user_id "ebsUserId", u.user_name "ebsUserName",
assmt.position_id "PositionId", assmt.job_id "jobId", assmt.grade_id "gradeId", assmt.organization_id "departmentId", loc.location_id "locationId",
mgr.person_id "managerId", mgr.full_name "managerName", mgr.email_address "managerEmail",
pos.name "PositionName", j.name "jobName", g.name "gradeName", o.name "departmentName", loc.location_code "locationCode", loc.description "locationDescription",
j.attribute1 "ArabicJobName",
addr.postal_code "postalCode", addr.address_line1 "addressLine1", addr.address_line2 "addressLine2", addr.address_line3 "addressLine3", addr.country "country", addr.town_or_city "cityCode", c_lkups.meaning "city",
(select max(group_name) from pay_people_groups where PEOPLE_GROUP_ID = assmt.PEOPLE_GROUP_ID) "peopleGroup",
''ACTIVE'' "active", p.CURRENT_EMPLOYEE_FLAG "isEmployee"
, XX_ZAIN_OCI_PKG.IS_CEO(p.person_id) "isCEO", XX_ZAIN_OCI_PKG.IS_MANAGER(p.person_id) "isManager", XX_ZAIN_OCI_PKG.IS_GM(p.person_id) "isGeneralManager"
from per_all_people_f p
join fnd_user u
on (p.person_id = u.employee_id)
left join PER_ALL_ASSIGNMENTS_F assmt
on (p.person_id = assmt.person_id and sysdate between nvl(assmt.effective_start_date, sysdate) and nvl(assmt.effective_end_date, sysdate+1000))
left join per_all_people_f mgr
on (assmt.supervisor_id = mgr.person_id and sysdate between nvl(mgr.effective_start_date, sysdate) and nvl(mgr.effective_end_date, sysdate+1000))
left join hr_all_positions_f pos
on (assmt.position_id = pos.position_id and sysdate between nvl(pos.effective_start_date, sysdate) and nvl(pos.effective_end_date, sysdate+1000))
left join per_jobs j
on (assmt.job_id = j.job_id and sysdate between j.date_from and nvl(j.date_to, sysdate+1000))
left join per_grades g
on (assmt.grade_id = g.grade_id and sysdate between g.date_from and nvl(g.date_to, sysdate+1000))
left join hr_all_organization_units o
on (assmt.organization_id = o.organization_id and sysdate between o.date_from and nvl(o.date_to, sysdate+1000))
left join hr_locations_all loc
on (o.location_id = loc.location_id)
--left join per_phones phn
--on (p.person_id=phn.parent_id)
left join per_addresses addr
on (p.person_id = addr.person_id and addr.primary_flag = ''Y'' and sysdate between addr.date_from and nvl(addr.date_to, sysdate+1000))
left join fnd_lookup_values_vl c_lkups
on (c_lkups.lookup_type = ''SA_CITY'' and addr.town_or_city = c_lkups.lookup_code)
where sysdate between p.effective_start_date and p.effective_end_date
and lower(u.user_name) = nvl(lower(#USER_NAME#), lower(u.user_name))
and u.EMAIL_ADDRESS = nvl(#EMAIL_ADDRESS#, u.EMAIL_ADDRESS)
and p.employee_number = nvl(#EMPLOYEE_NUMBER#, p.employee_number)','p.PERSON_ID|p.EMPLOYEE_NUMBER|u.EMAIL_ADDRESS|p.FIRST_NAME|p.LAST_NAME|u.USER_NAME|p.FULL_NAME');
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getRequestMetaData','select
    column_seq_num seqNum,
    replace(initcap(end_user_column_name), '' '', '''') "apiName",
    application_column_name "columnName",
    form_left_prompt "label",
    flex_value_set_id "flexValuesetId",
    nvl((select vs.apiname from xxx_zain_ess_valuesets vs
        where vs.FLEX_VALUE_SET_ID = d.flex_value_set_id), '''') "valueSetName",
    REQUIRED_FLAG "Required"
FROM
    fnd_descr_flex_col_usage_vl d
WHERE
    ( application_id = 800 )
    AND ( descriptive_flexfield_name = ''Extra Person Info DDF'' )
    AND ( descriptive_flex_context_code = ''#REQUEST_CODE#'' )
    and ENABLED_FLAG = ''Y''
ORDER BY
    column_seq_num',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getEmpAbsHistory','SELECT
    absence_type,
    absence_category,
    occurrence,
    absence_reason,
    date_notification,
    date_projected_start,
    time_projected_start,
    date_projected_end,
    time_projected_end,
    date_start,
    time_start,
    date_end,
    time_end,
    absence_days,
    absence_hours,
    person_id
FROM
    per_absence_attendances_v3
WHERE
    person_id = nvl(#PERSON_ID#, person_id)
    and absence_type = nvl(#ABSENCE_TYPE#, absence_type)
    and date_start >= nvl(#DATE_START#, date_start)
    and date_end <= nvl(#DATE_END#, date_end)
    -- person_id, absence_type, start_date, end_date
ORDER BY
    absence_reason,
    date_start DESC',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getPersonHierarchy','select per.person_id "personId", per.full_name "fullName", pos.name "positionName"
, per.sex "gender"
from per_all_people_f per
join per_all_assignments_f paaf
ON (  per.person_id = paaf.person_id
AND SYSDATE BETWEEN paaf.effective_start_date
AND paaf.effective_end_date
AND paaf.ASSIGNMENT_STATUS_TYPE_ID <> 3)
join hr_all_positions_f pos
on (paaf.position_id = pos.position_id
and sysdate between pos.effective_start_date and pos.effective_end_date)
where per.person_id = #PER_PERSON_ID#
and sysdate between per.effective_start_date and per.effective_end_date
union all
SELECT paaf.person_id "personId", per.full_name "fullName", pos.name "positionName"
, per.sex "gender"
FROM (SELECT POSITION_ID, count_persons
FROM (    SELECT LEVEL,
PPSE.Parent_Position_Id POSITION_ID,
(SELECT COUNT (DISTINCT paaf.person_id)
FROM per_all_assignments_f paaf
WHERE     paaf.position_id = PPSE.Parent_Position_Id
AND SYSDATE BETWEEN paaf.effective_start_date
AND paaf.effective_end_date
AND paaf.ASSIGNMENT_STATUS_TYPE_ID <> 3) count_persons
FROM PER_POS_STRUCTURE_ELEMENTS PPSE,
PER_POSITION_STRUCTURES PPS,
PER_POS_STRUCTURE_VERSIONS PPSV
WHERE     PPSV.POSITION_STRUCTURE_ID =
PPS.POSITION_STRUCTURE_ID
AND PPSV.POS_STRUCTURE_VERSION_ID =
PPSE.POS_STRUCTURE_VERSION_ID
AND PPSV.POSITION_STRUCTURE_ID = 1065
AND PPS.PRIMARY_POSITION_FLAG = ''Y''
CONNECT BY     PRIOR PPSE.Parent_Position_Id =
PPSE.SUBORDINATE_POSITION_ID
AND PRIOR PPSV.POS_STRUCTURE_VERSION_ID =
PPSV.POS_STRUCTURE_VERSION_ID
START WITH PPSE.SUBORDINATE_POSITION_ID =
(SELECT paaf.position_id
FROM per_all_assignments_f paaf
WHERE     paaf.person_id = #PER_PERSON_ID#
AND SYSDATE BETWEEN paaf.effective_start_date
AND paaf.effective_end_date)
ORDER BY 1)
WHERE count_persons > 0 --AND ROWNUM <= 1
) Ps
JOIN per_all_assignments_f paaf
ON (    paaf.position_id = Ps.position_id
AND SYSDATE BETWEEN paaf.effective_start_date
AND paaf.effective_end_date
AND paaf.ASSIGNMENT_STATUS_TYPE_ID <> 3)
join per_all_people_f per
on (paaf.person_id = per.person_id
and sysdate between per.effective_start_date and per.effective_end_date)
join hr_all_positions_f pos
on (paaf.position_id = pos.position_id
and sysdate between pos.effective_start_date and pos.effective_end_date)',null);
Insert into APPS.XXX_ZAIN_ESS_ENTITY_QUERIES (ENTITY_KEY,ENTITY_QUERY_VCHAR,COLS_FILTER) values ('getParkingDtls','SELECT ATTRIBUTE3 "parkingNumber", ATTRIBUTE2 "parkingColor", ATTRIBUTE1 "floor" FROM fnd_lookup_values where LOOKUP_TYPE  =  ''XX_RESERVED_PARKING''','MEANING');
