            meaning
        FROM
            hr_lookups h
        WHERE
                lookup_type = ''PHONE_TYPE''
            AND enabled_flag = ''Y''
            AND application_id = 800
            AND lookup_code = phone_type
            AND ( enabled_flag = ''Y''
                  OR end_date_active IS NULL )
    ) "phone_type",
    phone_number,
    date_from,
    date_to,
    parent_id,
    phone_id,
    object_version_number
FROM
    per_phones
WHERE
        parent_table = ''PER_ALL_PEOPLE_F''
    AND parent_id = #PER_PERSON_ID#
',null);
